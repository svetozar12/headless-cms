{
  "name": "envs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/envs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/envs",
        "main": "libs/envs/src/index.ts",
        "tsConfig": "libs/envs/tsconfig.lib.json",
        "assets": ["libs/envs/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/envs/**/*.ts"]
      }
    }
  },
  "tags": []
}
